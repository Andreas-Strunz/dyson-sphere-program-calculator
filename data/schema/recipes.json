{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "array",
    "properties": {
        "Name": {
            "type": "string"
        },
        "FacilityType": {
            "type": "string",
            "enum": [
                "Assembler",
                "Smelter"
            ]
        },
        "ProductionTime": {
            "type": "number"
        },
        "MainRecipe": {
            "type": "string"
        }
    },
    "required": [
        "FacilityType",
        "Name"
    ]
}