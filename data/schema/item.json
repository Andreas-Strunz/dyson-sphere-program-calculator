{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "array",
    "properties": {
        "Name": {
            "type": "string"
        },
        "Icon": {
            "type": "string",
            "format": "uri"
        },
        "Type": {
            "type": "string",
            "enum": [
                "Building",
                "Item",
                "Resource"
            ]
        },
        "Row": {
            "type": "integer",
            "minimum": 1,
            "maximum": 8
        },
        "Column": {
            "type": "integer",
            "minimum": 1,
            "maximum": 14
        }
    },
    "required": [
        "Name"
    ]
}